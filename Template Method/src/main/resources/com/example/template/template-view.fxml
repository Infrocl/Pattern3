<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.template.TemplateController">
   <children>
      <VBox layoutX="-3.0" layoutY="-3.0" prefHeight="500.0" prefWidth="500.0">
         <children>
            <HBox alignment="CENTER" prefHeight="60.0" prefWidth="500.0" spacing="100.0">
               <children>
                  <Button fx:id="startButton" mnemonicParsing="false" onAction="#onStartButtonClicked" text="Пуск">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="stopButton" mnemonicParsing="false" onAction="#onStopButtonClicked" text="Закрыть">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
